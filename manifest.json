{
  "manifest_version": 3,
  "name": "Tracker animeflv episodes",
  "version": "1.1.0",
  "description": "A Chrome extension to track episodes on anime flv",
  "permissions": ["storage", "tabs", "scripting"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["background.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "af.png"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
